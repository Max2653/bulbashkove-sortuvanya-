#include <iostream>
#include <cstdlib>
#include <ctime>
#include <algorithm>

int main() {
    int n;
    std::cin >> n;
    int arr[n];

    srand(time(0));
    for (int i = 0; i < n; i++) {
        arr[i] = rand() % 100 + 1;
        std::cout << arr[i] << " ";
    }

    std::cout << "\n";
    std::sort(arr, arr + n);

    for (int i = 0; i < n; i++) {
        std::cout << arr[i] << " ";
    }
}
